<?xml version="1.0"?>
<root main_tree_to_execute="BehaviorTree">
    <!-- ////////// -->
    <BehaviorTree ID="BehaviorTree">
        <Sequence name="Go deliver passenger">
            <Sequence name="Pick up passenger">
                <Sequence name="Navigate to passenger">
                    <Action ID="MoveUp"/>
                    <Action ID="MoveRight"/>
                </Sequence>
            </Sequence>
            <Sequence name="Drop off passenger">
                <Sequence name="Navigate to dropoff">
                    <Action ID="MoveDown"/>
                    <Action ID="MoveLeft"/>
                </Sequence>
            </Sequence>
        </Sequence>
    </BehaviorTree>
    <!-- ////////// -->
    <TreeNodesModel>
        <Action ID="MoveDown"/>
        <Action ID="MoveLeft"/>
        <Action ID="MoveRight"/>
        <Action ID="MoveUp"/>
    </TreeNodesModel>
    <!-- ////////// -->
</root>

